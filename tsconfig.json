{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "lib",
    "strict": true,
    "noImplicitAny": false,
    "declaration": true,
    "incremental": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "types": [ "@types/jest", "node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "lib": [
      "dom",
      "es5",
      "es2015.collection",
      "es2015.iterable"
    ]
  },
  "exclude": [
    "node_modules"
  ],
   "typedocOptions": {
    "out": "docs",
    "name": "Notional Finance V2 SDK",
    "entryPoints": [
      "src"
    ],
    "includeVersion": true,
    "exclude": "tests/**/*.ts",
    "excludePrivate": true,
    "excludeProtected": true,
    "hideBreadcrumbs": true
  }
}