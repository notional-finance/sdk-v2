{
  "name": "@notional-finance/sdk-v2",
  "version": "0.1.0",
  "description": "Notional Finance SDK V2",
  "author": "Jeff Wu <jeff@notional.finance>",
  "homepage": "https://notional.finance",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "notional",
    "defi",
    "ethereum"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notional-finance/sdk-v2.git"
  },
  "scripts": {
    "clean": "rm -Rf coverage lib node_modules tsconfig.build.tsbuildinfo",
    "typechain": "typechain --target=ethers-v5 --outDir src/typechain \"./src/abi/*.json\"",
    "build": "yarn run typechain && tsc -b tsconfig.build.json && yarn run lint",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --quiet --fix",
    "testenv:governance": "npx ts-node tests/governanceProposals.ts",
    "testenv:up": "cd ../subgraph-v2 && ./start.sh",
    "testenv:down": "cd ../subgraph-v2 && docker-compose down",
    "test:unit": "yarn run build && yarn run jest --selectProjects unit",
    "test:integration": "yarn run build && yarn run testenv:up && yarn run jest --selectProjects integration && yarn run testenv:down",
    "test": "yarn run build && yarn run testenv:up && yarn run jest && yarn run testenv:down"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^15.6.1",
    "@types/set-interval-async": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.3.4",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^27.2.5",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typechain": "^4.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "abi-decoder": "^2.4.0",
    "bnc-sdk": "^3.3.4",
    "cross-fetch": "^3.0.6",
    "ethers": "5.4.4",
    "eventemitter3": "^4.0.7",
    "graphql": "^15.3.0",
    "set-interval-async": "^2.0.3"
  },
  "engines": {
    "node": ">14.7.0"
  }
}
